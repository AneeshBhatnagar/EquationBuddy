<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="Aneesh" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/AneeshActionBar</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AneeshActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">#192823</item>
        <item name="android:titleTextStyle">@style/AneeshText</item>
    </style>
    <style name="AneeshText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#ebb035</item>
        <item name="android:textStyle">bold</item>
    </style>
    <color name="text">#218559</color>
    <color name="black">#000000</color>
    <color name="bgcolor">#FFFFFF</color>
    <color name="splashbg">#1f244a</color>

</resources>
